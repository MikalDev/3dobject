          "play-animation": {
            "list-name": "Play animation",
            "display-text": "Play animation [b]{0}[/b], [i](is looping {1})[/i]",
            "description": "Play animation by name.",
            "params": {
                  "animation": {
                      "name": "Animation",
                      "desc": "The animation name to play."
                  },
                  "loop": {
                    "name": "Loop",
                    "desc": "Loop animation."
                }
              }
          },
          "stop-animation": {
            "list-name": "Pause animation",
