              "axis": {
                "name": "Axis",
                "desc": "Axis to rotate object around.",
                "items": {
                  "X": "X",
                  "Y": "Y",
                  "Z": "Z"
                }
              }
            }
          },
          "set-z-elevation-0": {
            "list-name": "Set z elevation",
            "display-text": "Set z elevation to [i]{0}[/i]",
            "description": "Set z elevation (world z axis, depth component).",
            "params": {
                  "z-elevation": {
                      "name": "Zelevation",
                      "desc": "Zelevation (z axis, depth)"
                  }
              }
