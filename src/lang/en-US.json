              "y": {
                "name": "Y",
                "desc": "Y angle of rotation."
              },
              "z": {
                "name": "Z",
                "desc": "Z angle of rotation."
              },
              "rotation-order": {
                "name": "Rotation order",
                "desc": "Order to rotate object around axes.",
                "items": {
                  "xyz": "XYZ",
                  "xzy": "XZY",
                  "yxz": "YXZ",
                  "yzx": "YZX",
                  "zxy": "ZXY",
                  "zyx": "ZYX"
                }
              }
            }
